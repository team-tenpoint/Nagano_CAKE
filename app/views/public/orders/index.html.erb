<div class="container">
<div class="row">
<h1>注文履歴一覧</h1>
 <table class="table">
  <thead>
   <tr>
     <th>注文日</th>
     <th>配送先</th>
     <th>注文商品</th>
     <th>支払金額</th>
     <th>ステータス</th>
     <th>注文詳細</th>
    </tr>
  </thead>
    <% @orders.each do |order| %>
     <tbody>
       <tr>
         <td><%#= order.created_at.strftime %></td>
         <td>
          〒<%= order.postal_code %>
          <%= order.delivery_address %>
          <%= order.delivery_name %>
         </td>
         <td><%= cart_item.item_id %></td>
         <td><%= (cart_item.item.price_without_tax * 1.1).floor * (cart_item.quantity) %>円</td>
         <td><#= order_status %></td>
       </tr>
     <% end %>
    </tbody>
      <td><%#= link_to "表示する", order_path(@order.id), class: "btn btn-primary" %></td>
 </table>
 </div>
 </div>